record(stringin, "Group4:GssMtr:Model:get")
{
    field(DESC, "Returns the details of the device")
    field(PINI, "YES")
    field(DTYP, "stream")
    field(INP, "@gauss.proto getModel IP1")
    field (PHAS, "0")

}
record (ai, "Group4:GssMtr:MagX:get"){
    field(DESC, "Returns the Magnitude of X axis")
    field(PINI, "YES")
    field(DTYP, "stream")
    field(INP, "@gauss.proto getXValue IP1")
    field(SCAN, "1 second")
    field(PREC, "4")
    field (PHAS, "1")
}
